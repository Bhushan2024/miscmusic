@if(Context.User.Identity.Name != null)
{
    <form asp-controller="Account" asp-action="LogOut" id="logoutForm" method="post">
        <ul class="nav navbar-nav navbar-right">
            <li><a class="nav-link text-dark" asp-controller="Account" asp-action="Profile">Hello, @Context.User.Identity.Name</a></li>
            <li><button type="submit" class="nav-link btn btn-link text-dark">Logout</button></li>
        </ul>
        </form>
}
else{
    <ul class="nav navbar-nav navbar-right">
        <li><a class="nav-link text-dark" asp-controller="Account" asp-action="Registration">Register</a></li>
        <li><a class="nav-link text-dark" asp-controller="Account" asp-action="Login">Login</a></li>
    </ul>

}